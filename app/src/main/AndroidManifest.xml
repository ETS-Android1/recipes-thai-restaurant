<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.recipesthairestaurant">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RecipesThaiRestaurant">
        <activity android:name=".Activites.Privacy" android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>
        <activity android:name=".Activites.FullView" />
        <activity android:name=".Activites.Dashboard" />
        <activity android:name=".Activites.MainActivity" />
        <activity
            android:name=".Activites.SplashScreen"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>